{
    "name": "stereo",
    "displayName": "stereo",
    "description": "First VS CODE Extension",
    "version": "0.0.1",
    "publisher": "noname",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.speakUp"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.speakUp",
                "title": "Speak Up"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "say": "^0.14.0"
    }
}
